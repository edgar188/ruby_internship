!!!
%html
  %head
    %title RailsDemo
    %meta{content: 'width=device-width,initial-scale=1', name: 'viewport'}/
    = csrf_meta_tags
    = csp_meta_tag
    = stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_pack_tag 'application', 'data-turbolinks-track': 'reload'
    %script{src: 'https://ajax.googleapis.com/ajax/libs/jquery/3.2.0/jquery.min.js'}
    %script{src: 'https://unpkg.com/axios/dist/axios.min.js'}
    %link{href: 'https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css', rel: 'stylesheet'}/

    -# Age Range
    -# %link{:href => "https://demos.jquerymobile.com/1.4.2/css/themes/default/jquery.mobile-1.4.2.min.css", :rel => "stylesheet"}/
    -# %script{:src => "https://demos.jquerymobile.com/1.4.2/js/jquery.js"}
    -# %script{:src => "https://demos.jquerymobile.com/1.4.2/js/jquery.mobile-1.4.2.min.js"}


  %body
    = render 'shared/menu'
    = render 'shared/messages'
    %section.py-1
      .container.px-4.px-lg-5.mt-5
        = yield
    %script{src: 'https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js'}